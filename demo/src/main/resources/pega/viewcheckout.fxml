<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="500.0" prefWidth="500.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pega.ViewCheckoutController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <AnchorPane prefHeight="493.0" prefWidth="496.0">
         <children>
            <Button fx:id="btCheckin" layoutX="175.0" layoutY="260.0" mnemonicParsing="false" onAction="#btCheckin" text="Efetuar Check out" textAlignment="CENTER" textFill="#970b0b">
               <font>
                  <Font name="Arial Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="btVoltarCliente" layoutX="14.0" layoutY="423.0" mnemonicParsing="false" onAction="#btVoltarCliente" text="Voltar" textFill="#970b0b">
               <font>
                  <Font name="Arial Bold" size="12.0" />
               </font>
            </Button>
            <TextField fx:id="txtCPF" layoutX="161.0" layoutY="200.0" prefHeight="36.0" prefWidth="138.0" promptText="CPF:" />
            <TextField fx:id="txtNome" layoutX="161.0" layoutY="150.0" prefHeight="36.0" prefWidth="138.0" promptText="Nome:" />
            <Label layoutX="182.0" layoutY="121.0" text="Dados do cliente" textFill="#970b0b">
               <font>
                  <Font name="Arial Bold" size="12.0" />
               </font>
            </Label>
            <Label layoutX="188.0" layoutY="52.0" text="Check out" textFill="#970b0b">
               <font>
                  <Font name="Arial Bold" size="20.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</VBox>
