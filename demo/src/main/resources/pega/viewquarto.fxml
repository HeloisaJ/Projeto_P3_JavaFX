               <font>
                  <Font name="Arial Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="btVoltarlogin" layoutX="14.0" layoutY="423.0" mnemonicParsing="false" onAction="#btVoltarlogin" text="Voltar tela inicial" textFill="#970b0b">
               <font>
                  <Font name="Arial Bold" size="12.0" />
               </font>
            </Button>
            <TextField fx:id="tipoCama" alignment="CENTER" layoutX="185.0" layoutY="129.0" prefHeight="36.0" prefWidth="138.0" promptText="solteiro/casal" />
            <Label layoutX="159.0" layoutY="35.0" text="Checagem de quartos" textFill="#970b0b">
               <font>
                  <Font name="Arial Bold" size="18.0" />
