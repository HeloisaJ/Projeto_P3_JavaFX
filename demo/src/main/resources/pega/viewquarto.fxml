            <Label layoutX="90.0" layoutY="129.0" prefHeight="36.0" prefWidth="138.0" text="# tipo do quarto =" textAlignment="CENTER" textFill="#f01d07">
               <font>
                  <Font name="Arial Italic" size="12.0" />
               </font>
            </Label>
            <Label fx:id="Chave" layoutX="288.0" layoutY="292.0" text="Label" textFill="#f01d07" />
            <Label layoutX="171.0" layoutY="283.0" prefHeight="36.0" prefWidth="138.0" text="chave encontrada =" textAlignment="CENTER" textFill="#f01d07">
               <font>
                  <Font name="Arial Italic" size="12.0" />
               </font>
            </Label>
            <Button fx:id="registrarClient" layoutX="192.0" layoutY="318.0" mnemonicParsing="false" onAction="#onBtregistrarClient" prefHeight="23.0" prefWidth="124.0" text="Registrar cliente " textAlignment="CENTER" textFill="#970b0b">
               <font>
                  <Font name="Arial Bold" size="12.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</VBox>
